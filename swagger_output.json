    "/members/manage": {
      "post": {
        "tags": [
          "Members-front"
        ],
        "summary": "取得所有會員 (開發查詢用)",
        "description": "取得所有會員資料 (方便開發查詢用) <br>  body 需帶入管理員密碼",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "管理者密碼",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "adminPassword": {
